{
  "manifest_version": 2,
  "name": "JIRA Assist",
  "version": "0.0.0.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "img/logo-32.png",
    "128": "img/logo-32.png"
  },
  "permissions": [
    "*://www.softomate.net/jira/*"
  ],
  "content_security_policy": "default-src 'self'; connect-src *; style-src 'self' 'unsafe-inline'",
  "app": {
	"launch": {
		"local_path": "index.html"
	}
  }
}