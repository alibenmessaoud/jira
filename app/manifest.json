{
  "manifest_version": 2,
  "name": "JIRA Assist",
  "version": "0.0.0.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "img/logo-32.png",
    "128": "img/logo-128.png"
  },
  "permissions": [
    "<all_urls>", "storage", "webview"
  ],
  "content_security_policy": "default-src 'self'; connect-src *; style-src 'self' 'unsafe-inline'; img-src *",
  "app": {
    "background": {
      "scripts": [
        "js/libs/jquery-1.11.0.min.js",
        "js/libs/moment.min.js",
        "js/libs/underscore-min.js",
        "js/libs/backbone-min.js",
        "js/models.js",
        "js/jira.js",
        "app.js"
      ]
    }
  }
}